    "def count(lst):\n",
    "    \n",
    "    even=0\n",
    "    odd=0\n",
    "    for i in lst:\n",
    "        if i%2==0:\n",
    "            even+=1\n",
    "        else:\n",
    "            odd+=1\n",
    "    return even,odd       \n",
    "            \n",
    "lst=[1,2,3,7,5,6]\n",
    "\n",
    "even,odd=count(lst)\n",
    "\n",
    "print(even)\n",
    "print(odd)"
   ]
